<div class="row">
    <div class="col-md-12">
        <div class="card strpied-tabled-with-hover">
            <div class="card-header ">
                <h4 class="card-title">QueroSerTech Integration </h4>
                <p class="card-category">Youtube Videos</p>
            </div>
            <div class="card-body table-full-width table-responsive">
                <table class="table table-hover table-striped">
                    <thead>
                        <th>Title</th>
                        <th>Link</th>
                        <th>Channel</th>
                        <th>thumb</th>
                        <th>Actions</th>
                    </thead>
                    <tbody>

                        {{#each data.videos}}

                        <tr>
                            <td>{{this.title}}</td>
                            <td>
                                <a href="{{this.link}}" target="_blank">Link</a>
                            </td>
                            <td>
                                <a href="{{this.channel_link}}" target="_blank">{{this.channel_name}}</a>
                            </td>
                            <td>

                                <a class="btn btn-default btn-simple btn-link"  target="_blank" href="{{this.thumbnail}}">
                                    <i class="fa fa-file-image-o"></i>
                                </a>

                            </td>
                            <td>

                                <a class="btn btn-success btn-simple btn-link" href="/proxy/scrapy/video/{{this._id}}/accept">
                                    <i class="fa fa-check"></i>
                                </a>
                                <a class="btn btn-danger btn-simple btn-link" href="/proxy/scrapy/video/{{this._id}}/deny">
                                    <i class="fa fa-times"></i>
                                </a>
                            </td>
                        </tr>

                        {{/each}}

                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>